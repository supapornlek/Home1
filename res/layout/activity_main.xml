<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/image"
    android:gravity="center_horizontal"
    tools:context="${relativePackage}.${activityClass}" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="156dp"
            android:layout_height="133dp"
            android:scaleType="fitXY"
            android:src="@drawable/f" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/linearLayout1"
        android:layout_marginRight="19dp"
        android:orientation="vertical" >

        <Button
            android:id="@+id/btn11"
            android:layout_width="151dp"
            android:layout_height="69dp"
            android:text="Edit Profile" />

        <Button
            android:id="@+id/btn12"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:text="View Image" />
    </LinearLayout>

    <TextView
        android:id="@+id/tex12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/linearLayout1"
        android:layout_below="@+id/tex11"
        android:layout_marginTop="10dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex12"
        android:layout_below="@+id/tex12"
        android:layout_marginTop="22dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex14"
        android:layout_below="@+id/tex14"
        android:layout_marginTop="26dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex20"
        android:layout_below="@+id/tex20"
        android:layout_marginTop="26dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tex14"
        android:layout_alignRight="@+id/linearLayout2"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tex14"
        android:layout_alignRight="@+id/tex13"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tex16"
        android:layout_alignBottom="@+id/tex16"
        android:layout_alignRight="@+id/tex15"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tex18"
        android:layout_alignRight="@+id/tex17"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tex20"
        android:layout_alignRight="@+id/tex19"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tex24"
        android:layout_alignBottom="@+id/tex24"
        android:layout_alignLeft="@+id/tex23"
        android:text="TextView" />

    <View
        android:id="@+id/view1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_above="@+id/tex14"
        android:layout_centerHorizontal="true"
        android:background ="#cccccc" />

    <View
        android:id="@+id/view4"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignBottom="@+id/tex19"
        android:layout_alignParentLeft="true"
        android:background="#cccccc" />

    <View
        android:id="@+id/view3"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tex16"
        android:background="#cccccc" />

    <View
        android:id="@+id/view2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignBottom="@+id/tex14"
        android:layout_alignParentLeft="true"
        android:background="#cccccc" />

    <View
        android:id="@+id/view5"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_above="@+id/tex22"
        android:layout_alignParentLeft="true"
        android:background="#cccccc" />

    <TextView
        android:id="@+id/tex18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex16"
        android:layout_below="@+id/view3"
        android:layout_marginTop="27dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex18"
        android:layout_below="@+id/view4"
        android:layout_marginTop="27dp"
        android:text="TextView" />

    <View
        android:id="@+id/view6"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_below="@+id/tex22"
        android:background="#cccccc" />

    <TextView
        android:id="@+id/tex23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex21"
        android:layout_alignTop="@+id/tex22"
        android:text="TextView" />

    <View
        android:id="@+id/view7"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignBottom="@+id/tex25"
        android:layout_alignParentLeft="true"
        android:background="#cccccc" />

    <TextView
        android:id="@+id/tex24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tex22"
        android:layout_below="@+id/view6"
        android:layout_marginTop="30dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tex11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="TextView" />

</RelativeLayout>